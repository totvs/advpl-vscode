                "icon": {
                    "light": "images/light/build.svg",
                    "dark": "images/dark/build.svg"
                }
            },
            {
                "command": "advpl.applyPatchFile",
                "title": "%advpl.applyPatchFile%",
                "icon": {
                    "light": "images/light/package.svg",
                    "dark": "images/dark/package.svg"
                }
            },
            {
                "command": "advpl.whatsNew",
                "title": "Advpl - What's New"
            }
        ],
        "keybindings": [
            {
                "command": "advpl.compile",
                "key": "ctrl+f9",
                "mac": "cmd+f9",
                "when": "editorLangId == advpl"
            },
            {
                "command": "advpl.compile",
                "key": "ctrl+f9",
                "mac": "cmd+f9",
                "when": "editorLangId == 4gl"
            },
            {
                "command": "advpl.startSmartClient",
                "key": "ctrl+f11",
                "mac": "cmd+f11",
                "when": "editorLangId == advpl"
            },
            {
                "command": "advpl.createPPO",
                "key": "ctrl+shift+f9",
                "mac": "cmd+f4",
                "when": "editorLangId == advpl"
            },
            {
                "command": "advpl.compileFilesOpened",
                "key": "alt+f9",
                "mac": "alt+f9",
