{
    "name": "advpl-vscode",
    "displayName": "Advpl",
    "description": "%advpl.package.description%",
    "version": "0.14.10",
    "publisher": "KillerAll",
    "icon": "images/logo_vscode.png",
    "homepage": "https://github.com/totvs/advpl-vscode",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages",
        "Formatters",
        "Debuggers",
        "Snippets"
    ],
    "capabilities": {
        "documentFormattingProvider": "true"
    },
    "activationEvents": [
        "onLanguage:advpl",
        "onLanguage:4gl",
        "onCommand:advpl.startSmartClient",
        "onCommand:advpl.menucompile",
        "onCommand:advpl.menucompileProjet",
        "onCommand:advpl.menucompilemulti",
        "onCommand:advpl.menucompiletextfile",
        "onCommand:advpl.compile",
        "onCommand:advpl.compileFilesOpened",
        "onCommand:advpl.applyPatchFile",
        "onCommand:advpl.getProgramName",
        "onCommand:advpl.getDebugInfos",
        "onCommand:advpl.getAuthorizationId",
        "onCommand:advpl.CipherPassword",
        "onCommand:advpl.patch.apply",
        "onCommand:advpl.patch.build",
        "onCommand:advpl.patch.info",
        "onCommand:advpl.monitor.getThreads",
        "onCommand:advpl.monitor.getRpoInfos",
        "onCommand:advpl.monitor.getRpoFunctions",
        "onCommand:advpl.monitor.deleteSource",
        "onCommand:advpl.monitor.deleteSourceContext",
        "onCommand:advpl.monitor.defragRpo",
        "onCommand:advpl.buildWSClient",
        "onCommand:advpl.selectEnvironment",
        "onCommand:advpl.whatsNew",
        "onCommand:advpl.serversManagement.AddServer",
        "onCommand:advpl.addAdvplEnvironment"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "replay-time",
                    "title": "Replay",
                    "icon": "images/replay.png"
                }
            ]
        },
        "views": {
            "replay-time": [
                {
                    "id": "replayTimeLine",
                    "name": "Replay TimeLine"
                }
            ],
            "explorer": [
                {
                    "id": "serversManagement",
                    "name": "%advpl.selectEnvironment.title%"
                }
            ]
        },
        "viewsWelcome": [
            {
                "view": "serversManagement",
                "contents": "No environments found [learn more](https://github.com/totvs/advpl-vscode/wiki/Criação-de-ambiente-facilitada).\n[Add Environment](command:advpl.serversManagement.AddServer)"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "advpl.menucompile",
                    "group": "advpl"
                },
                {
                    "command": "advpl.menucompilemulti",
                    "when": "explorerResourceIsFolder",
                    "group": "advpl"
                },
                {
                    "command": "advpl.menucompileProjet",
                    "when": "resourceFilename =~ /.*\\.prj/i",
                    "group": "advpl"
                },
                {
                    "command": "advpl.menucompiletextfile",
                    "when": "resourceFilename =~ /.*\\.txt/i",
                    "group": "advpl"
                },
                {
                    "command": "advpl.patch.apply",
                    "when": "resourceFilename =~ /.*\\.ptm/i",
                    "group": "advpl"
                },
                {
                    "command": "advpl.patch.info",
                    "when": "resourceFilename =~ /.*\\.ptm/i",
                    "group": "advpl"
                },
                {
                    "command": "advpl.patch.build",
                    "when": "resourceFilename =~ /.*\\.txt/i",
                    "group": "advpl"
                },
                {
                    "command": "advpl.monitor.deleteSourceContext",
                    "group": "advpl"
                }
            ],
            "view/item/context": [
                {
                    "command": "advpl.serversManagement.connect",
                    "when": "view == serversManagement && viewItem == Environment"
                },
                {
                    "command": "advpl.serversManagement.DeleteEnvironment",
                    "when": "view == serversManagement && viewItem == Environment",
                    "group": "inline"
                },
                {
                    "command": "advpl.serversManagement.Defrag",
                    "when": "view == serversManagement && viewItem == EnvironmentConnected"
                },
                {
                    "command": "advpl.serversManagement.getRpoInfos",
                    "when": "view == serversManagement && viewItem == EnvironmentConnected"
                },
                {
                    "command": "advpl.serversManagement.getRpoFunctions",
                    "when": "view == serversManagement && viewItem == EnvironmentConnected"
                },
                {
                    "command": "advpl.applyPatchFile",
                    "when": "view == serversManagement && viewItem == EnvironmentConnected"
                },
                {
                    "command": "advpl.serversManagement.rename",
                    "when": "view == serversManagement"
                },
                {
                    "command": "advpl.serversManagement.getAllEnvironments",
                    "when": "view == serversManagement && viewItem == ServiceConnected"
                },
                {
                    "command": "advpl.serversManagement.CipherPassword",
                    "when": "view == serversManagement && viewItem =~ /^Environment/i"
                }
            ],
            "view/title": [
                {
                    "command": "advpl.serversManagement.AddServer",
                    "when": "view == serversManagement",
                    "group": "navigation"
                }
            ],
            "editor/title": [
                {
                    "command": "advpl.compileFilesOpened",
                    "group": "advpl",
                    "when": "resourceLangId == advpl"
                },
                {
                    "command": "advpl.compileFilesOpened",
                    "group": "navigation",
                    "when": "resourceLangId == advpl"
                },
                {
                    "command": "advpl.applyPatchFile",
                    "group": "navigation"
                }
            ],
            "editor/title/context": [
                {
                    "command": "advpl.compileFilesOpened",
                    "group": "advpl",
                    "when": "resourceLangId == advpl"
                }
            ],
            "editor/context": [
                {
                    "command": "advpl.compileFilesOpened",
                    "group": "advpl",
                    "when": "resourceLangId == advpl"
                }
            ],
            "commandPalette": [
                {
                    "command": "advpl.serversManagement.connect",
                    "when": "config.noExists"
                },
                {
                    "command": "advpl.serversManagement.DeleteEnvironment",
                    "when": "config.noExists"
                },
                {
                    "command": "advpl.serversManagement.Defrag",
                    "when": "config.noExists"
                },
                {
                    "command": "advpl.serversManagement.getRpoInfos",
                    "when": "config.noExists"
                },
                {
                    "command": "advpl.serversManagement.getRpoFunctions",
                    "when": "config.noExists"
                },
                {
                    "command": "advpl.serversManagement.rename",
                    "when": "config.noExists"
                },
                {
                    "command": "advpl.serversManagement.getAllEnvironments",
                    "when": "config.noExists"
                },
                {
                    "command": "advpl.serversManagement.CipherPassword",
                    "when": "config.noExists"
                },
                {
                    "command": "advpl.patch.apply",
                    "when": "config.noExists"
                },
                {
                    "command": "advpl.monitor.deleteSourceContext",
                    "when": "config.noExists"
                },
                {
                    "command": "advpl.menucompile",
                    "when": "config.noExists"
                }
            ]
        },
        "configurationDefaults": {
            "[advpl]": {
                "files.encoding": "windows1252"
            },
            "[4gl]": {
                "files.encoding": "windows1252"
            }
        },
        "configuration": {
            "type": "object",
            "title": "%advpl.configuration.title%",
            "properties": {
                "advpl.selectedEnvironment": {
                    "type": "string",
                    "default": "",
                    "description": "%advpl.configuration.properties.ADVPLSELECTEDENVIRONMENT%"
                },
                "advpl.enableCoverage": {
                    "type": "boolean",
                    "default": "true",
                    "description": "%advpl.configuration.properties.ADVPLENABLECOVERAGE%"
                },
                "advpl.workspaceFolders": {
                    "type": "string",
                    "default": "",
                    "description": "%advpl.configuration.properties.ADVPLWORKSPACEFOLDERS%"
                },
                "advpl.environments": {
                    "type": "array",
                    "default": [],
                    "description": "%advpl.configuration.properties.ADVPLENVIRONMENTS%",
                    "items": {
                        "type": "object",
                        "properties": {
                            "totvs_language": {
                                "type": "string",
                                "enum": [
                                    "advpl",
                                    "4gl"
                                ],
                                "default": "advpl",
                                "description": "language"
                            },
                            "smartClientPath": {
                                "type": "string",
                                "default": "",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.SMARTCLIENTPATH%"
                            },
                            "name": {
                                "type": "string",
                                "default": null,
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.NAME%"
                            },
                            "serverVersion": {
                                "type": "string",
                                "enum": [
                                    "131227A",
                                    "170117A",
                                    "191205P",
                                    "210324P"
                                ],
                                "default": "170117A",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.SERVERVERSION%"
                            },
                            "server": {
                                "type": "string",
                                "default": "localhost",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.SERVER%"
                            },
                            "port": {
                                "type": "string",
                                "default": "1201",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.PORT%"
                            },
                            "environment": {
                                "type": "string",
                                "default": "environment",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.ENVIRONMENT%"
                            },
                            "user": {
                                "type": "string",
                                "default": "Admin",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.USER%"
                            },
                            "passwordCipher": {
                                "type": "string",
                                "default": "",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.PASSWORDCIPHER%"
                            },
                            "language": {
                                "type": "string",
                                "default": "PORTUGUESE",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.LANGUAGE%"
                            },
                            "rpoType": {
                                "type": "string",
                                "default": "TOP",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.RPOTYPE%"
                            },
                            "includeList": {
                                "type": "string",
                                "default": "",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.INCLUDELIST%"
                            },
                            "enable": {
                                "type": "boolean",
                                "default": true,
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.ENABLE%"
                            },
                            "defragPackAllInfo": {
                                "type": "boolean",
                                "default": true,
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.DEFRAGPACK%"
                            },
                            "webappPort": {
                                "type": "string",
                                "default": "8486",
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.WEBAPPPORT%"
                            },
                            "ssl": {
                                "type": "boolean",
                                "default": false,
                                "description": "%advpl.contributes.configuration.properties.ADVPLENVIRONMENTS.items.properties.SSL%"
                            }
                        }
                    }
                },
                "advpl.startProgram": {
                    "type": "string",
                    "default": "SIGAFAT",
                    "description": "%advpl.configuration.properties.ADVPLSTARTPROGRAM%"
                },
                "advpl.authorization_generation": {
                    "type": "string",
                    "default": "",
                    "pattern": "\\d{8}",
                    "description": "Data de geração da chave no formato yyyymmdd"
                },
                "advpl.authorization_validation": {
                    "type": "string",
                    "default": "",
                    "pattern": "\\d{8}",
                    "description": "Data de validade da chave no formato yyyymmdd"
                },
                "advpl.authorization_permission": {
                    "type": "string",
                    "default": "",
                    "description": "1 - Para permitir substiuir fontes TOTVS, 0 para Não"
                },
                "advpl.authorization_code": {
                    "type": "string",
                    "default": "",
                    "description": "Chave de compilação"
                },
                "advpl.compileToken": {
                    "type": "string",
                    "default": "",
                    "description": "Token de Compilação, caso informado, será ignorado a chave de autorização antiga"
                },
                "advpl.compileFolderRegex": {
                    "type": "string",
                    "default": ".*\\.(prw|prx|prg|apw|aph|tres|png|bmp|res|apl|tlpp|4gl|per|msg|ahu|app)",
                    "description": "%advpl.configuration.properties.ADVPLCOMPILEFOLDERREGEX%"
                },
                "advpl.pathPatchBuild": {
                    "type": "string",
                    "default": "c:\\patchs\\",
                    "description": "%advpl.configuration.properties.ADVPLPATHPATCHBUILD%"
                },
                "advpl.debug_multiThread": {
                    "type": "boolean",
                    "default": false,
                    "description": "%advpl.configuration.properties.ADVPLDEBUG_MULTITHREAD%"
                },
                "advpl.debug_ignoreSourceNotFound": {
                    "type": "boolean",
                    "default": false,
                    "description": "%advpl.configuration.properties.ADVPLDEBUG_IGNORESOURCENOTFOUND%"
                },
                "advpl.debug_showTables": {
                    "type": "boolean",
                    "default": false,
                    "description": "%advpl.configuration.properties.ADVPLDEBUG_SHOWTABLES%"
                },
                "advpl.debug_showPrivates": {
                    "type": "boolean",
                    "default": false,
                    "description": "%advpl.configuration.properties.ADVPLDEBUG_SHOWPRIVATES%"
                },
                "advpl.debug_showPublic": {
                    "type": "boolean",
                    "default": false,
                    "description": "%advpl.configuration.properties.ADVPLDEBUG_SHOWPUBLIC%"
                },
                "advpl.debug_showStatics": {
                    "type": "boolean",
                    "default": false,
                    "description": "%advpl.configuration.properties.ADVPLDEBUG_SHOWSTATICS%"
                },
                "advpl.debug_inspect_macro": {
                    "type": "boolean",
                    "default": false,
                    "description": "%advpl.configuration.properties.ADVPLDEBUG_INSPECT_MACRO%"
                },
                "advpl.compile_force_recompile": {
                    "type": "boolean",
                    "default": false,
                    "description": "Caso esteja como true, força recompilar os fontes mesmo que não tiveram alteração."
                },
                "advpl.logger": {
                    "type": "boolean",
                    "default": false,
                    "description": "%advpl.configuration.properties.ADVPLLOGGER%"
                },
                "advpl.compile_threads": {
                    "type": "number",
                    "default": 4,
                    "description": "%advpl.configuration.properties.ADVPLCOMPILE_THREADS%"
                },
                "advpl.alpha_compile": {
                    "type": "boolean",
                    "default": false,
                    "description": "%advpl.configuration.properties.ADVPLCOMPILE_ALPHA%"
                },
                "advpl.dictionary": {
                    "type": "array",
                    "default": [],
                    "description": "%advpl.configuration.properties.ADVPLDICTIONARY%",
                    "items": {
                        "type": "object",
                        "properties": {
                            "parent": {
                                "type": "string",
                                "default": "",
                                "description": "%advpl.contributes.configuration.properties.DICTIONARY.items.properties.PARENT%"
                            },
                            "name": {
                                "type": "string",
                                "default": "",
                                "description": "%advpl.contributes.configuration.properties.DICTIONARY.items.properties.NAME%"
                            },
                            "label": {
                                "type": "string",
                                "default": "",
                                "description": "%advpl.contributes.configuration.properties.DICTIONARY.items.properties.LABEL%"
                            }
                        }
                    }
                },
                "advplformat.noQueryFormatter": {
                    "type": "boolean",
                    "description": "%contributes.configuration.properties.advplformat.noQueryFormatter%"
                },
                "advplformat.queryLanguage": {
                    "type": "string",
                    "enum": [
                        "sql",
                        "n1ql",
                        "db2",
                        "pl/sql"
                    ],
                    "enumDescriptions": [
                        "Standard SQL",
                        "Couchbase N1QL",
                        "IBM DB2",
                        "Oracle PL/SQL"
                    ],
                    "default": "sql",
                    "description": "%contributes.configuration.properties.advplformat.queryLanguage%"
                }
            }
        },
        "languages": [
            {
                "id": "advpl",
                "extensions": [
                    ".prw",
                    ".prx",
                    ".prg",
                    ".aph",
                    ".apl",
                    ".tlpp",
                    ".ahu",
                    ".apw"
                ],
                "aliases": [
                    "Advpl(PRW)",
                    "Advpl(PRX)",
                    "Advpl(PRG)",
                    "Advpl(APH)",
                    "Advpl(APL)",
                    "TLPP(tlpp)",
                    "Advpl(AHU)",
                    "Advpl(APW)"
                ],
                "configuration": "./advpl.configuration.json"
            },
            {
                "id": "4gl",
                "extensions": [
                    ".4gl",
                    ".per",
                    ".msg"
                ],
                "aliases": [
                    "4GL(4gl)"
                ],
                "configuration": "./4gl.configuration.json"
            },
            {
                "id": "advpl-tlpp-header",
                "extensions": [
                    ".ch",
                    ".th"
                ],
                "aliases": [
                    "Advpl Header(CH)",
                    "TLPP Header (TH)"
                ],
                "configuration": "./advpl-tlpp-header.configuration.json"
            }
        ],
        "breakpoints": [
            {
                "language": "advpl"
            }
        ],
        "grammars": [
            {
                "language": "advpl",
                "scopeName": "source.advpl",
                "path": "./syntaxes/advpl.tmLanguage.tmLanguage"
            },
            {
                "language": "4gl",
                "scopeName": "source.4gl",
                "path": "./syntaxes/4gl.tmLanguage.tmLanguage"
            },
            {
                "language": "advpl-tlpp-header",
                "scopeName": "source.advpl-tlpp-header",
                "path": "./syntaxes/advpl-tlpp-header.tmLanguage"
            }
        ],
        "commands": [
            {
                "command": "advpl.startSmartClient",
                "title": "%advpl.startSmartClient%"
            },
            {
                "command": "advpl.menucompile",
                "title": "%advpl.menucompile%"
            },
            {
                "command": "advpl.menucompilemulti",
                "title": "%advpl.menucompilemulti%"
            },
            {
                "command": "advpl.menucompileProjet",
                "title": "%advpl.menucompileProjet%"
            },
            {
                "command": "advpl.menucompiletextfile",
                "title": "%advpl.menucompiletextfile%"
            },
            {
                "command": "advpl.compile",
                "title": "%advpl.compile%"
            },
            {
                "command": "advpl.createPPO",
                "title": "%advpl.createPPO%"
            },
            {
                "command": "advpl.getDebugInfos",
                "title": "%advpl.getDebugInfos%"
            },
            {
                "command": "advpl.getDebugPath",
                "title": "Path"
            },
            {
                "command": "advpl.getAuthorizationId",
                "title": "%advpl.getAuthorizationId%"
            },
            {
                "command": "advpl.CipherPassword",
                "title": "%advpl.CipherPassword%"
            },
            {
                "command": "advpl.patch.apply",
                "title": "%advpl.patch.apply%"
            },
            {
                "command": "advpl.patch.build",
                "title": "%advpl.patch.build%"
            },
            {
                "command": "advpl.patch.info",
                "title": "%advpl.patch.info%"
            },
            {
                "command": "advpl.monitor.getThreads",
                "title": "%advpl.monitor.getThreads%"
            },
            {
                "command": "advpl.monitor.getRpoInfos",
                "title": "%advpl.monitor.getRpoInfos%"
            },
            {
                "command": "advpl.monitor.getRpoFunctions",
                "title": "%advpl.monitor.getRpoFunctions%"
            },
            {
                "command": "advpl.monitor.deleteSource",
                "title": "%advpl.monitor.deleteSource%"
            },
            {
                "command": "advpl.monitor.deleteSourceContext",
                "title": "%advpl.monitor.deleteSource%"
            },
            {
                "command": "advpl.monitor.defragRpo",
                "title": "%advpl.monitor.defragRpo%"
            },
            {
                "command": "advpl.buildWSClient",
                "title": "%advpl.buildWSClient%"
